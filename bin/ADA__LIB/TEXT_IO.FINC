if defined NEW_LINE_L26_
PRO	NEW_LINE_L26				;---------- PRO NEW_LINE
PRMS					;    debut parametrage
	PRM SPACING_ofs				; in
endPRMS					;    fin parametrage
ELB 1					;    BODY ELAB
					;    end elab
begin:					;---------- BDY INSTRUCTIONS
	LI	 13
	CALL	TEXT_IO. ,PUT_L52
	LI	 1
	Sd  0,	STDOUT_COL_disp
VAR	NL2_disp, d				; compteur boucle LOOP__1
	LI	 1
	Sd  1,	NL2_disp
VAR	LMT_NL2_disp, d				; limite boucle LOOP__1
	Ld  1,	-SPACING_ofs
	Sd  1,	LMT_NL2_disp
	Ld  1,	NL2_disp				; test null range LOOP__1
	Ld  1,	LMT_NL2_disp
	CGT
	BT	L1
LOOP__1:					; corps boucle LOOP__1
	LI	 10
	CALL	TEXT_IO. ,PUT_L52
	Ld  1,	NL2_disp				; test de sortie LOOP__1
	Ld  1,	LMT_NL2_disp
	CEQ
	BT	L1
	Ld  1,	NL2_disp				; mise a jour compteur LOOP__1
	INC
	Sd  1,	NL2_disp
	BRA	LOOP__1				; iteration suivante LOOP__1
L1:					; post loop LOOP__1
	Ld  0,	STDOUT_LINE_disp
	Ld  1,	-SPACING_ofs
	ADD
	Sd  0,	STDOUT_LINE_disp
					; debut if
	Ld  0,	STDOUT_LINE_disp
	Ld  0,	STDOUT_MAX_PAGE_LEN_disp
	CGT
	BF	L4
	LI	 12
	CALL	TEXT_IO. ,PUT_L52
	Ld  0,	STDOUT_PAGE_disp
	LI	 1
	ADD
	Sd  0,	STDOUT_PAGE_disp
	LI	 1
	Sd  0,	STDOUT_LINE_disp
	BRA	L3
L4:
L3:					; post if
	UNLINK 1
	RTD	prm_siz
excep:
endPRO					;---------- end PRO NEW_LINE
end if

