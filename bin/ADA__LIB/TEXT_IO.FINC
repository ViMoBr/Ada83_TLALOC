if defined NEW_LINE_L26_
PRO	NEW_LINE_L26				;---------- PRO NEW_LINE
PRMS					;    debut parametrage
	PRM SPACING_ofs				; in
endPRMS					;    fin parametrage
ELB 1					;    BODY ELAB
					;    end elab
begin:					;---------- BDY INSTRUCTIONS
	LI	 13
	CALL	STANDARD.TEXT_IO. ,PUT_L52
	LI	1
	Sd  0,	STDOUT_COL_disp
VAR	NL6_disp, d				; compteur boucle LOOP__1
	LI	1
	Sd  1,	NL6_disp
VAR	LMT_NL6_disp, d				; limite boucle LOOP__1
	Ld  1,	-SPACING_ofs
	Sd  1,	LMT_NL6_disp
	Ld  1,	NL6_disp				; test null range LOOP__1
	Ld  1,	LMT_NL6_disp
	CGT
	BT	L5
LOOP__1:					; corps boucle LOOP__1
	LI	 10
	CALL	STANDARD.TEXT_IO. ,PUT_L52
	Ld  1,	NL6_disp				; test de sortie LOOP__1
	Ld  1,	LMT_NL6_disp
	CEQ
	BT	L5
	Ld  1,	NL6_disp				; mise a jour compteur LOOP__1
	INC
	Sd  1,	NL6_disp
	BRA	LOOP__1				; iteration suivante LOOP__1
L5:					; post loop LOOP__1
	Ld  0,	STDOUT_LINE_disp
	Ld  1,	-SPACING_ofs
	ADD
	Sd  0,	STDOUT_LINE_disp
					; debut if
	Ld  0,	STDOUT_LINE_disp
	Ld  0,	STDOUT_MAX_PAGE_LEN_disp
	CGT
	BF	L8
	LI	 12
	CALL	STANDARD.TEXT_IO. ,PUT_L52
	Ld  0,	STDOUT_PAGE_disp
	LI	1
	ADD
	Sd  0,	STDOUT_PAGE_disp
	LI	1
	Sd  0,	STDOUT_LINE_disp
	BRA	L7
L8:
L7:					; post if
ret_lbl:
	UNLINK 1
	RTD	(prm_siz)
excep:
endPRO					;---------- end PRO NEW_LINE
end if

