	Ld  1,	LMT_IL6_disp
	CEQ
	BT	L5
	Ld  1,	IL6_disp				; mise a jour compteur LOOP__2
	INC
	Sd  1,	IL6_disp
	BRA	LOOP__2				; iteration suivante LOOP__2
L5:					; post loop LOOP__2
	La  1,	CHN_disp
	CALL	TEXT_IO. ,PUT_L56
	UNLINK 1
