if defined NEW_LINE_L25_
PRO	NEW_LINE_L25				;---------- PRO NEW_LINE
PRMS					;    debut parametrage
	PRM FILE_ofs				; in
	PRM SPACING_ofs				; in
endPRMS					;    fin parametrage
ELB 1					;    BODY ELAB
