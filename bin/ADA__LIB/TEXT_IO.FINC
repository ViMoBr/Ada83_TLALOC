namespace CHN				; array var constrained array type info
VAR SIZ, d
  namespace info
VAR COMP_SIZ, d
VAR FST_1, d
VAR LST_1, d
	LI	1
	Sd 1, COMP_SIZ
	Ld 1, COMP_SIZ
	LI	1
	Sd 1, FST_1
	LI	40
	Sd 1, LST_1
	Ld 1, LST_1
	INC
	Ld 1, FST_1
	SUB
	MUL
	Sd 1, SIZ
  end namespace
  virtual at 4
COMP_SIZ = $
	rd 1 
FST_1 = $
	rd 1 
LST_1 = $
	rd 1 
  end virtual
end namespace

VAR CHN_disp, q				; variable array : pointeur aux data
VAR CHN__u, q				; variable array : useinfo pointeur au rec info
	Ld 1, CHN.SIZ
	CO_VAR
	Sa 1, CHN_disp				; array data ptr at _disp
	LVA 1, 	CHN.SIZ
	Sa 1, CHN__u				; array info ptr at __u

VAR LEN_disp, d				; variable entiere
	LI	40
	Sd  1,	LEN_disp

VAR VAL_disp, d				; variable entiere
	LI	0
	Sd  1,	VAL_disp

					;    end elab
begin:					;---------- BDY INSTRUCTIONS
	LVA  1,	LEN_disp
	LVA  1,	CHN_disp
	CALL	STANDARD.TEXT_IO. ,GET_LINE_L58
VAR	IL10_disp, d				; compteur boucle LOOP__2
	LI	1
	Sd  1,	IL10_disp
VAR	LMT_IL10_disp, d			; limite boucle LOOP__2
	Ld  1,	LEN_disp
	Sd  1,	LMT_IL10_disp
	Ld  1,	IL10_disp				; test null range LOOP__2
	Ld  1,	LMT_IL10_disp
	CGT
	BT	L9
LOOP__2:					; corps boucle LOOP__2
	LI	10
	Ld  1,	VAL_disp
	MUL
	La 1,	CHN_disp				; array data start address on stack
	Ld  1,	IL10_disp
	LId 1,	CHN__u, CHN.FST_1			; (index - FST_1) * SIZ_1
	SUB
	LId 1,	CHN__u, CHN.COMP_SIZ
	MUL
	ADD					; add offset to start address
	Lb					; charge depuis adresse empilee 
	ADD
	LI	48
	SUB
	Sd  1,	VAL_disp
	Ld  1,	IL10_disp				; test de sortie LOOP__2
	Ld  1,	LMT_IL10_disp
	CEQ
	BT	L9
	Ld  1,	IL10_disp				; mise a jour compteur LOOP__2
	INC
	Sd  1,	IL10_disp
	BRA	LOOP__2				; iteration suivante LOOP__2
L9:					; post loop LOOP__2
	Ld  1,	VAL_disp
	SId  1,	-ITEM_ofs
	LVa  1,	CHN_disp
	CALL	STANDARD.TEXT_IO. ,PUT_L56
ret_lbl:
	UNLINK 1
	RTD	(prm_siz)
excep:
endPRO					;---------- end PRO GET
end if

