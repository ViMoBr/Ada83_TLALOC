;			---------
macro			PUT_CHAR?			; affiche un seul caractère sur stdout
;			---------

  db 0x48, 0x89, 0xE6				; mov rsi, rsp adresse du caractère empilé
  db 0x6A, 0x01					; push 1
  db 0x58						; pop rax      (rax=1=sys_write )
  db 0x48, 0x89, 0xC2				; mov rdx, rax (rdx=length=1)
  db 0x48, 0x89, 0xC7				; mov rdi, rax (rdi=stdout=1)
  db 0x0F, 0x05					; syscall
  DROP						; caractère consommé
end macro

;			--------
macro			PUT_STR?			; affiche tout une chaine sur stdout
;			--------
  db 0x48, 0x89, 0xC6				; mov rsi, rax  (esi=T  adresse des caracteres a afficher)
  db 0x8B, 0x50, 0xFC				; mov EDX, dword ptr[rax-4]   EDX=last
  db 0xFF, 0xC2					; inc EDX
  db 0x2B, 0x50, 0xF8				; sub EDX, dword ptr[rax-8]   EDX=length (=last - first)
  db 0x6A, 0x01					; push 1
  db 0x58						; pop rax     (rax=1=sys_write )
  db 0x48, 0x89,0xC7				; mov rdi,rax (rdi=stdout=1)
  db 0x0F, 0x05					; syscall
  DROP						; adresse consommée
end macro

;			---------
macro			GET_CHAR?			; lit un seul caractère sur stdin
;			---------

  db 0x5E						; pop rsi	     (l'adresse destination a ete empilee)
  db 0x6A, 0x00					; push 0
  db 0x58						; pop rax      (rax=0=sys_read )
  db 0x48, 0x89, 0xC7				; mov rdi, rax (rdi=stdout=0)
  db 0x6A, 0x01					; push 1
  db 0x5A						; pop rdx (rdx=length=1)
  db 0x0F, 0x05					; syscall
  DROP						; adresse consommée
end macro


;			--------
macro			SYSEXIT?			; retour au systeme
;			--------
  db 0x6A, 0x3C					; push x3C  (sys_exit rax=60)
  db 0x58						; pop rax
  db 0x31, 0xFF					; xor edi, edi (err_code=0)
  db 0x0F, 0x05					; syscall

end macro

